<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


	<grid name="student-grid" title="Student"
		model="com.axelor.student.db.Student">
		<field name="stdName" />
		<field name="stdPh" />
		<field name="stdAdds" />
		<field name="stdDept" />
		<field name="stdSub" />
	</grid>
	<form model="com.axelor.student.db.Student" title="Students" name="student-form">
	<panel title="StudentView">
	<field name="stdName" ></field>
	<field name="stdPh" canNew="true"></field>
	<field name="stdAdds" colSpan="4"></field>
	<field name="stdDept" colSpan="4" canNew="true"></field>
	<field name="stdSub"  colSpan="4"></field>
	</panel>
	</form>

</object-views>